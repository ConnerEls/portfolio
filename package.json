{
  "private": true,
  "name": "odinvr",
  "description": "Odin VR Application",
  "version": "1.0.0",
  "author": "Conner Els",
  "main": "main/background.js",
  "scripts": {
    "dev": "nextron",
    "build": "electron-builder",
    "build-next": "next build ./renderer",
    "export": "next export",
    "postinstall": "electron-builder install-app-deps"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.529.0",
    "@aws-sdk/lib-storage": "^3.564.0",
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.2",
    "@babel/preset-react": "^7.24.7",
    "@chakra-ui/react": "^2.8.2",
    "@emotion/react": "^11.13.0",
    "@emotion/styled": "^11.13.0",
    "@fortawesome/fontawesome-svg-core": "^6.6.0",
    "@fortawesome/free-solid-svg-icons": "^6.6.0",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "@mui/icons-material": "^5.16.4",
    "@mui/material": "^5.16.4",
    "@mui/styles": "^5.16.6",
    "@nextui-org/avatar": "^2.0.30",
    "@nextui-org/react": "^2.4.2",
    "@nextui-org/system": "^2.2.2",
    "@nextui-org/theme": "^2.2.6",
    "@reduxjs/toolkit": "^2.2.6",
    "@zip.js/zip.js": "^2.7.52",
    "adm-zip": "^0.5.14",
    "antd": "^5.19.4",
    "axios": "^1.7.2",
    "babel-loader": "^9.1.3",
    "bootstrap": "^5.3.3",
    "electron-serve": "^1.3.0",
    "electron-store": "^8.2.0",
    "extract-zip": "^2.0.1",
    "fast-xml-parser": "^4.4.1",
    "fflate": "^0.8.2",
    "framer-motion": "^11.3.19",
    "jsonwebtoken": "^9.0.2",
    "jszip": "^3.10.1",
    "moment": "^2.30.1",
    "mui": "^0.0.1",
    "prop-types": "^15.8.1",
    "react": "^18.3.1",
    "react-avatar": "^5.0.3",
    "react-big-calendar": "^1.15.0",
    "react-bootstrap": "^2.10.5",
    "react-calendar": "^5.0.0",
    "react-confirm-alert": "^3.0.6",
    "react-datepicker": "^7.4.0",
    "react-dom": "^18.3.1",
    "react-icons": "^5.2.1",
    "react-rating-stars-component": "^2.2.0",
    "react-redux": "^9.1.2",
    "react-router-dom": "^6.24.1",
    "react-select": "^5.8.1",
    "react-star-rating-component": "^1.4.1",
    "react-toastify": "^10.0.5",
    "redux": "^4.2.1",
    "redux-devtools-extension": "^2.13.9",
    "redux-logger": "^3.0.6",
    "redux-promise-middleware": "^6.2.0",
    "redux-thunk": "^3.1.0",
    "tailwindcss": "^3.4.4",
    "urldecode": "^1.0.1",
    "uuid": "^10.0.0",
    "xmlbuilder": "^15.1.1"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "electron": "^31.0.1",
    "electron-builder": "^24.13.3",
    "next": "^14.2.4",
    "nextron": "^9.1.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "build": {
    "appId": "com.odinvr.app",
    "productName": "OdinVR",
    "directories": {
      "buildResources": "build",
      "output": "dist/"
    },
    "files": [
      "dist/",
      "main/",
      "renderer/",
      "package.json"
    ],
    "win": {
      "requestedExecutionLevel": "requireAdministrator",
      "target": "nsis",
      "icon": "build/icon.ico"
    },
    "mac": {
      "target": "dmg",
      "icon": "build/icon.icns"
    },
    "nsis": {
      "oneClick": false,
      "perMachine": true,
      "allowElevation": true,
      "allowToChangeInstallationDirectory": false,
      "createDesktopShortcut": true,
      "script": "build/installer.nsh"
    }
  }
}